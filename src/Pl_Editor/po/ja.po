#. type: Title ~
#: Pl_Editor.adoc:185
#, no-wrap
msgid "Format symbols:"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:188
msgid "Texts can be simple strings or can include format symbols."
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:190
msgid "Format symbols are replaced by the actual values in Eeschema or Pcbnew."
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:192
msgid "They are like format symbols in printf function."
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:194
msgid "A format symbol is *%* followed by 1 letter."
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:196
msgid "The *%C* format has one digit (comment identifier)"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:198
msgid "Formats symbols are:"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:200
#, no-wrap
msgid "*%% = replaced by %*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:202
#, no-wrap
msgid "*%K = KiCad version*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:204
#, no-wrap
msgid "*%Z = paper format name (A4, USLetter ...)*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:206
#, no-wrap
msgid "*%Y = company name*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:208
#, no-wrap
msgid "*%D = date*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:210
#, no-wrap
msgid "*%R = revision*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:212
#, no-wrap
msgid "*%S = sheet number*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:214
#, no-wrap
msgid "*%N = number of sheets*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:216
#, no-wrap
msgid "*%Cx = comment (x = 0 to 9 to identify the comment)*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:218
#, no-wrap
msgid "*%F = filename*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:220
#, no-wrap
msgid "*%P = sheet path (sheet full name, for Eeschema)*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:222
#, no-wrap
msgid "*%T = title*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:224
msgid "Example:"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:226
msgid "\"Size: %Z\" displays \"Size: A4\" or \"Size: USLetter\""
